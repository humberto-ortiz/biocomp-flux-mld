	Number = {9},
	Pages = {1579 --1589},
	Title = {Homeotic gene action in embryonic brain development of Drosophila},
	Url = {http://dev.biologists.org/content/125/9/1579.abstract},
	Urldate = {2011-01-26},
	Volume = {125},
