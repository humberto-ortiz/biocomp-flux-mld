	Issn = {1943-2631},
	Journal = {Genetics},
	Keywords = {Actins, Animals, Base Sequence, Cytoskeleton, Drosophila melanogaster, Enzyme Activation, Epidermis, Gene Expression Regulation, Gene Targeting, Genes, Insect, Genes, Reporter, Genetic Testing, {JNK} Mitogen-Activated Protein Kinases, Larva, {MAP} Kinase Signaling System, Models, Biological, Regulatory Sequences, Nucleic Acid, {RNA} Interference, Time Factors, Transgenes, Wound Healing},
	Month = nov,
	Note = {{PMID:} 20813879},
	Number = {3},
	Pages = {943--957},
	Title = {A targeted {UAS-RNAi} screen in Drosophila larvae identifies wound closure genes regulating distinct cellular processes},
	Volume = {186},
