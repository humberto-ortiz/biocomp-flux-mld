	Journal = {Nature},
	Keywords = {Animals, Animals, Genetically Modified, Drosophila melanogaster, Exons, Female, Genomic Library, Male, Muscles, Neurons, Organ Specificity, Ribonuclease {III}, {RNA} Interference, {RNA}, Messenger},
	Month = jul,
	Note = {{PMID:} 17625558},
	Number = {7150},
	Pages = {151--156},
	Title = {A genome-wide transgenic {RNAi} library for conditional gene inactivation in Drosophila},
	Volume = {448},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nature05954}}

