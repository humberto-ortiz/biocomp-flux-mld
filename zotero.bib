	Keywords = {Animals, Antibody Specificity, Chromatography, Affinity, Drosophila melanogaster, Ganglia, Hu Paraneoplastic Encephalomyelitis Antigens, Nervous System, Neuroglia, Neurons, Retina, Ribonucleoproteins, Staining and Labeling, Subcellular Fractions},
	Month = jul,
	Note = {{PMID:} 1716300},
	Number = {5},
	Pages = {443--461},
	Title = {Characterization and spatial distribution of the {ELAV} protein during Drosophila melanogaster development},
