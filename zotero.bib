	Issn = {1098-5336},
	Journal = {Applied and environmental microbiology},
	Keywords = {Animals, Bacteria, Biodiversity, Birds, Crop, Avian, {DNA}, Bacterial, {DNA}, Ribosomal, Molecular Sequence Data, Phylogeny, {RNA}, Bacterial, {RNA}, Ribosomal, {16S}, Sequence Analysis, {DNA}, Sequence Homology, Nucleic Acid},
	Month = oct,
	Note = {{PMID:} 18689523},
